{
  "name": "project3",
  "version": "1.0.0",
  "description": "a blog built with react",
  "main": "server.js",
  "engines": {
    "node": "v12.12.0",
    "npm": "6.13.6"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/agomez99/CodeSource.git"
  },
  "author": "Austine Gomez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agomez99/CodeSource/issues"
  },
  "homepage": "https://github.com/agomez99/CodeSource#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "nodemon": "^2.0.3"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}
